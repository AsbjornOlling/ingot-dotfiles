
# Keyboard backlight up
"kbdlight up 10"
    m:0x0 + c:238
    XF86KbdBrightnessUp

# Keyboard backlight down
"kbdlight down 10"
    m:0x0 + c:237
    XF86KbdBrightnessDown


# Screen brightness up
"~/Scripts/brightness.sh up"
    m:0x0 + c:233
    XF86MonBrightnessUp

# Screen brightness down
"~/Scripts/brightness.sh down"
    m:0x0 + c:232
    XF86MonBrightnessDown


#Volume down
"pamixer --decrease 5"
    m:0x0 + c:122
    XF86AudioLowerVolume

#Volume up
"pamixer --increase 5"
    m:0x0 + c:123
    XF86AudioRaiseVolume

#Toggle mute
"pamixer --toggle-mute"
    m:0x0 + c:121
    XF86AudioMute

